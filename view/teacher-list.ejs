<!DOCTYPE html>
<html>

<head>
    <%- include ('partials/head');%>
</head>
<%- include("partials/loader")%>
    <%- include ('partials/header');%>
        <div id="tt-pageContent">
            
        </div>
        <div class="container-indent">
            <div class="container container-fluid-custom-mobile-padding">
                <h1 class="tt-title-subpages noborder">Tài khoản</h1>
                <%if (userList != null){ userList.forEach(function(item) {%>
                    <div class="tt-wrapper" style="margin-bottom: 50px;">
                        <div class="tt-table-responsive">
                            <table class="tt-table-shop-02">
                                <tbody>
                                    <tr>
                                        <td>Username:</td>
                                        <td>
                                            <%= item.username %>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>E-MAIL:</td>
                                        <td>
                                            <%= item.email %>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Password:</td>
                                        <td>
                                            <%= item.password %>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <a href="/teacher-list/edit/<%= item.id%>" class="btn btn-border">Change</a>
                        <a href="/teacher-list/delete/<%= item._id %>" class="btn btn-border">Delete</a>
                    </div>
                    <% });} %>
            </div>
        </div>
        </div>
        <%- include("partials/script") %>
            <footer>
                <%- include('partials/footer'); %>
            </footer>

</html>